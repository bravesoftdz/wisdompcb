  object qry1: TADOQuery
    Connection = ADOConnection1
    CommandTimeout = 60
    Parameters = <
      item
        Name = 'dtpk1'
        DataType = ftDateTime
        Value = Null
      end
      item
        Name = 'dtpk2'
        DataType = ftDateTime
        Value = Null
      end
      item
        Name = 'dtpk3'
        Size = -1
        Value = Null
      end
      item
        Name = 'dtpk4'
        Size = -1
        Value = Null
      end>
    SQL.Strings = (
      ' select distinct data0010.cust_code'
      '  from Data0064'
      'inner join data0439 on data0439.rkey =data0064.packing_list_ptr'
      'inner join Data0060 on data0060.rkey=data0064.SO_PTR'
      'inner join Data0010 on data0439.customer_ptr=data0010.RKEY '
      'left join Data0097 on data0097.RKEY =data0060.PURCHASE_ORDER_PTR'
      'inner join Data0025 on data0060.CUST_PART_PTR=data0025.RKEY '
      'left join Data0008 on data0025.PROD_CODE_PTR=data0008.RKEY'
      ' left join (select SO_PTR,SUM(AMOUNT) as amount from Data0062 '
      ' group by SO_PTR)a on a.SO_PTR=data0060.rkey'
      'WHERE     (dbo.data0439.date_sailing >= :dtpk1) and '
      '                   (dbo.data0439.date_sailing <= :dtpk2) '
      'union'
      'select distinct data0010.cust_code     '
      'FROM         dbo.Data0098 LEFT OUTER JOIN'
      
        '                      dbo.Data0060 ON dbo.Data0098.SO_PTR = dbo.' +
        'Data0060.RKEY INNER JOIN'
      
        '                      dbo.Data0010 ON dbo.Data0098.CUSTOMER_PTR ' +
        '= dbo.Data0010.RKEY INNER JOIN'
      
        '                      dbo.Data0025 ON dbo.Data0098.CUSTOMER_PART' +
        '_PTR = dbo.Data0025.RKEY INNER JOIN'
      
        '                      dbo.Data0015 ON dbo.Data0025.PROD_ROUTE_PT' +
        'R = dbo.Data0015.RKEY LEFT OUTER JOIN'
      
        '                      dbo.Data0097 ON dbo.Data0060.PURCHASE_ORDE' +
        'R_PTR = dbo.Data0097.RKEY LEFT OUTER JOIN'
      
        '                      dbo.Data0015 AS Data0015_1 ON dbo.Data0060' +
        '.SHIP_REG_TAX_ID = Data0015_1.RKEY left join '
      
        '                      Data0008 on data0025.PROD_CODE_PTR=data000' +
        '8.RKEY'
      
        'where (data0098.rma_date>=:dtpk3) and (data0098.rma_date<=:dtpk4' +
        ')    ')
    Left = 360
    Top = 168
