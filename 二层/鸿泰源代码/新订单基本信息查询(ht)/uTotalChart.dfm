      object Memo1: TMemo
        Left = 48
        Top = 136
        Width = 553
        Height = 225
        Lines.Strings = (
          
            'SELECT TOP 100 PERCENT cc.total_ship / dd.PARTS_ORDERED * 100 AS' +
            ' dcy, '
          '      dd.PARTS_ORDERED, Data0010.ABBR_NAME'
          'FROM Data0060 INNER JOIN'
          
            '          (SELECT TOP 100 PERCENT CUSTOMER_PTR, SUM(total_ship) ' +
            'AS total_ship'
          
            '         FROM (SELECT TOP 100 PERCENT SUM(Data0064.QUAN_SHIPPED)' +
            ' '
          
            '                       / Data0060.PARTS_ORDERED * SUM(Data0064.Q' +
            'UAN_SHIPPED) '
          '                       AS total_ship, Data0060.PARTS_ORDERED, '
          '                       Data0060.CUSTOMER_PTR'
          '                 FROM Data0064 INNER JOIN'
          
            '                       Data0060 ON Data0064.SO_PTR = Data0060.RK' +
            'EY AND '
          '                       CONVERT(datetime, CONVERT(varchar, '
          'Data0064.DATE_ASSIGN, 101), '
          '                       101) <= Data0060.SCH_DATE'
          '                 WHERE (Data0064.assign_type = 0) AND ')
        TabOrder = 1
        Visible = False
      end
      object Memo2: TMemo
        Left = 48
        Top = 24
        Width = 601
        Height = 89
        Lines.Strings = (
          ' GROUP BY Data0060.SALES_ORDER, Data0060.PARTS_ORDERED, '
          '                       Data0060.CUSTOMER_PTR) d1'
          '         GROUP BY CUSTOMER_PTR'
          '         ORDER BY CUSTOMER_PTR DESC) cc ON '
          '      Data0060.CUSTOMER_PTR = cc.CUSTOMER_PTR INNER JOIN'
          
            '          (SELECT TOP 100 PERCENT SUM(PARTS_ORDERED) AS PARTS_OR' +
            'DERED, '
          '               CUSTOMER_PTR'
          
            '         FROM (SELECT TOP 100 PERCENT SUM(Data0064.QUAN_SHIPPED)' +
            ' '
          
            '                       / Data0060.PARTS_ORDERED * SUM(Data0064.Q' +
            'UAN_SHIPPED) '
          '                       AS total_ship, Data0060.PARTS_ORDERED, '
          '                       Data0060.CUSTOMER_PTR'
          '                 FROM Data0064 INNER JOIN'
          
            '                       Data0060 ON Data0064.SO_PTR = Data0060.RK' +
            'EY AND '
          
            '                       CONVERT(datetime, CONVERT(varchar, Data00' +
            '64.DATE_ASSIGN, 101), '
          '                       101) <= Data0060.SCH_DATE'
          '                 WHERE (Data0064.assign_type = 0) AND ')
        TabOrder = 2
        Visible = False
      end
      object Memo3: TMemo
        Left = 96
        Top = 192
        Width = 577
        Height = 113
        Lines.Strings = (
          ' GROUP BY Data0060.SALES_ORDER, Data0060.PARTS_ORDERED, '
          '                       Data0060.CUSTOMER_PTR) d1'
          '         GROUP BY CUSTOMER_PTR) dd ON '
          '      Data0060.CUSTOMER_PTR = dd.CUSTOMER_PTR INNER JOIN'
