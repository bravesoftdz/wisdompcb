      '034 AS d34 ON D56.DEPT_PTR = d34.RKEY INNER JOIN'#13#10'          dbo.' +
      'data0492 AS D492 ON D492.CUT_NO = D06.CUT_NO INNER JOIN'#13#10'       ' +
      '   dbo.Data0008 AS D08 ON D08.RKEY = d25.PROD_CODE_PTR INNER JOI' +
