      '.RKEY INNER JOIN'#13#10'          dbo.Data0010 AS D10 ON D492.COMPLETE' +
      'D = D10.RKEY ON D60.SALES_ORDER = D492.SO_NO LEFT OUTER JOIN'#13#10'  ' +
