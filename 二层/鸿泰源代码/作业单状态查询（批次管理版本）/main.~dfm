      'SELECT     D06.RKEY, D06.PANEL_A_B, ROUND(d25.unit_sq * D56.QTY_' +
      'BACKLOG, 4) AS Qty_SMQ, D06.wtype AS wtype06, d34.DEPT_CODE, d34' +
      '.DEPT_NAME, d34.WO_DAYS, '#13#10#9#9#9'  d34.DEPT_PTR AS deptptr, d34.TTY' +
      'PE, D06.QUAN_SCH, D56.FLOW_NO, D06.WORK_ORDER_NUMBER, D06.PROD_S' +
      'TATUS, D06.QUAN_REJ, DATEDIFF(minute, '#13#10#9#9#9'  D56.INTIME, GETDATE' +
      '()) / 60.00 AS TimeDiff, D06.QUAN_PROD, D06.QUAN_ALLREJ, D56.INT' +
      'IME, D06.REFERENCE_PTR, d25.PARENT_PTR, D06.PANELS, '#13#10#9#9#9'  D56.P' +
      'ANELS AS PNL_ONLine, D06.BOM_PTR, d25.MANU_PART_NUMBER, d25.MANU' +
      '_PART_DESC, D56.STEP, D56.RKEY AS d56_rkey, D56.WTYPE, '#13#10#9#9#9'  d2' +
      '5.SAMPLE_NR, D06.FLOW_NO AS Flow_NO_06, D06.SCH_COMPL_DATE, D56.' +
      'REFERENCE, D06.PARTS_PER_PANEL, D56.QTY_BACKLOG, D56.TO_BE_STOCK' +
      'ED, '#13#10#9#9#9'  D56.DEPT_PTR, D492.SO_NO, D492.CUT_NO, D492.analysis_' +
      'code_3, D08.PRODUCT_NAME, D10.CUST_CODE, D10.ABBR_NAME, D06.ENT_' +
      'DATETIME, d25.unit_sq, '#13#10#9#9#9'  CASE WHEN D06.PARTS_PER_PANEL <> 0' +
      ' THEN D06.panel_ln * D06.panel_wd * 0.000001 / D06.PARTS_PER_PAN' +
      'EL ELSE 0 END AS unit_sq_includingBoader, '#13#10#9#9#9'  D56.QTY_BACKLOG' +
      ' * d25.unit_sq AS prod_Square, CASE WHEN D06.PARTS_PER_PANEL <> ' +
      '0 THEN D56.QTY_BACKLOG * (D06.panel_ln * D06.panel_wd * 0.000001' +
      ') '#13#10#9#9#9'  / D06.PARTS_PER_PANEL ELSE 0 END AS prod_Square_includi' +
      'ngBoader, '#13#10#9#9#9'  CASE D06.Prod_Status WHEN 3 THEN '#39#29983#20135#20013#39' WHEN 4 T' +
      'HEN '#39#22806#21457#29983#20135#39' WHEN 5 THEN '#39#24453#20837#20179#39' WHEN 6 THEN '#39#24453#20998#37197#39' WHEN 103 THEN '#39#29983#20135 +
      #20013#26242#32531#39' ELSE '#39#39' END AS'#13#10#9#9#9'   prod_Status_CN, CASE isnull(D56.wType' +
      ', 0) WHEN 0 THEN '#39#39' ELSE '#39#26159#39' END AS OnlineStatus_Old, D60.so_old' +
      'new, D60.SALES_ORDER,D60.PARTS_ORDERED,'#13#10#9#9#9'  CASE D60.so_tp WHE' +
      'N 0 THEN '#39#33258#21046#39' WHEN 1 THEN '#39#22806#21457#39' WHEN 2 THEN '#39#21322#21046#31243#39' END AS sotp, D4' +
      '92.QTY_REJECT - (D492.ISSUED_QTY - D492.ORD_REQ_QTY) '#13#10#9#9#9'  * 0.' +
      '5 AS QTYREJECT, D60.REFERENCE_NUMBER, D492.WHOUSE_PTR, dbo.Data0' +
      '097.PO_NUMBER, d15.ABBR_NAME AS FacNm, d25.RKEY AS rkey25, '#13#10#9#9#9 +
      '  D492.QTY_REJECT - (D492.ISSUED_QTY - D492.ORD_REQ_QTY) AS QTYR' +
      'EJECT_1, '#13#10#9#9#9'  CASE D06.wtype WHEN 0 THEN '#39#27491#24120#39' WHEN 1 THEN '#39#34987#25286#20998 +
      #39' WHEN 2 THEN '#39#24453#36820#24037#39' WHEN 3 THEN '#39#36820#24037#39' WHEN 4 THEN '#39#24453#25253#24223#39' WHEN 5 TH' +
      'EN '#39#24050#25253#24223#39' ELSE '#39#39' END'#13#10#9#9#9'   AS OnlineStatus, D06.CUTNO_WO, CASE ' +
      'isnull(D56.PUT_TYPE, 0) WHEN 0 THEN '#39#39' ELSE '#39#26159#39' END AS ReCheck, ' +
      'D10_2.CUST_CODE AS ORIG_CUSTOMER, '#13#10#9#9#9'  d25.LAYERS, D07.PRODUCT' +
      '_GROUP_NAME, d25.CPJS,d60.PARTS_SHIPPED,(d60.PARTS_ORDERED-d60.P' +
      'ARTS_SHIPPED) as qianjiao_qty,b.QTY_REJECT,b.ISSUED_QTY'#13#10'FROM   ' +
      '   dbo.Data0010 AS D10_2 RIGHT JOIN'#13#10'          dbo.Data0060 AS D' +
      '60 ON D10_2.ABBR_NAME = D60.ORIG_CUSTOMER RIGHT OUTER JOIN'#13#10'    ' +
      '      dbo.Data0025 AS d25 INNER JOIN'#13#10'          dbo.Data0006 AS ' +
      'D06 ON D06.BOM_PTR = d25.RKEY INNER JOIN'#13#10'          dbo.Data0056' +
