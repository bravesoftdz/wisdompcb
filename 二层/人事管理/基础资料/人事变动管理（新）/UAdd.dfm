  object qry_Add: TADOQuery
    Connection = DM.con1
    CursorType = ctStatic
    LockType = ltBatchOptimistic
    Parameters = <>
    SQL.Strings = (
      
        'select pdet.train_flag,msg.employeecode,chinesename,Train_NO,Tra' +
        'in_topics,[status],datadetail.item rankname,[rank],'
      
        '(case sex when 0 then '#39#22899#39' when 1 then '#39#30007#39' else '#39#20854#23427#39'  end) as sex' +
        'y ,'
      
        'ondutytime,outdutytime,province,IDCard,departmentid,pdet.item as' +
        ' pposition,DG.item DormiGrade,'
      
        'mdet.item as employ_type,dept.departmentname,position,starLevel,' +
        'Data0005.EMPLOYEE_NAME,'
      'employee_alteration.* ,ods.item as stationitem, msg.station,'
      
        ' msg.IsKeyPostiton as IsKeyPosition , KeyP.positionName as  KeyP' +
        'ositionName,PG.item as PGrade,'
      'Lsight  as WYear'
      'from employee_alteration '
      
        'left join Employee_Train Tra on employee_alteration.employee_tra' +
        'in_ptr=Tra.rkey'
      
        'left join employeemsg AS msg ON msg.rkey = employee_alteration.e' +
        'mployeeid '
      'left join datadepartment dept on dept.rkey=msg.departmentid'
      ' left join datadetail pdet on pdet.rkey=msg.position'
      ' left join datadetail mdet on mdet.rkey=msg.employ_type'
      ' left join datadetail on datadetail.rkey=msg.[Rank]'
      ' left join datadetail ods on ods.rkey=msg.station'
      ' left join datadetail DG on DG.rkey=msg.DGrade_ptr'
      ' left join datadetail PG on PG.rkey = msg.PGrade'
      ' left join Data0005 ON Data0005.RKEY = oper_person '
      'left  join  hr_keyposition KeyP on  msg.keypid=keyp.rkey'
      'where 1=2')
    Left = 40
    Top = 89
