  object ado0022: TADOQuery
    Connection = Form1.ADOConnection1
    CursorType = ctStatic
    Filtered = True
    BeforeInsert = ado0022BeforeInsert
    BeforeEdit = ado0022BeforeEdit
    BeforeDelete = ado0022BeforeDelete
    Parameters = <>
    Prepared = True
    SQL.Strings = (
      
        'SELECT     TOP (100) PERCENT dbo.Data0017.INV_PART_NUMBER, dbo.D' +
        'ata0017.INV_NAME, dbo.Data0017.INV_DESCRIPTION, dbo.Data0496.GRO' +
        'UP_DESC, '
      
        '                      dbo.Data0022.RKEY AS rkey22, dbo.Data0023.' +
        'ABBR_NAME AS code23, dbo.Data0022.QUAN_ON_HAND, dbo.Data0022.QUA' +
        'N_TO_BE_STOCKED,                       '
      
        '                       (case when dbo.Data0022.EXPIRE_DATE IS no' +
        't NULL  then  DATEDIFF(day, GETDATE(), dbo.Data0022.EXPIRE_DATE)' +
        ' else 0 end ) AS eff_day,'
      
        '                      dbo.Data0022.QUAN_ON_HAND - dbo.Data0022.Q' +
        'UAN_TO_BE_STOCKED AS quan_allow, '
      
        '                      dbo.Data0001.CURR_CODE, dbo.Data0022.tax_p' +
        'rice, dbo.Data0016.LOCATION, dbo.Data0002.UNIT_NAME, dbo.Data002' +
        '2.EXPIRE_DATE,dbo.Data0022.BARCODE_ID, '
      
        '                      dbo.Data0022.cust_decl, dbo.Data0070.PO_RE' +
        'V_NO, dbo.Data0022.TAX_2, dbo.Data0456.GRN_NUMBER, dbo.Data0015.' +
        'WAREHOUSE_CODE, '
      
        '                      dbo.Data0456.ship_DATE, dbo.Data0022.rohs,' +
        ' dbo.Data0022.SUPPLIER2, dbo.Data0022.PRICE, '
      
        '                      ROUND(dbo.Data0022.QUAN_ON_HAND * dbo.Data' +
        '0017.STOCK_SELL, 4) AS total_area, dbo.Data0456.exch_rate, '
      
        '                      ROUND(dbo.Data0022.PRICE * dbo.Data0456.ex' +
        'ch_rate, 4) AS L_no_taxprice, dbo.Data0022.tax_price * dbo.Data0' +
        '456.exch_rate AS L_tax_price, '
      
        '                      ROUND((dbo.Data0022.tax_price * dbo.Data04' +
        '56.exch_rate * dbo.Data0022.QUAN_ON_HAND) / (1 + dbo.Data0022.TA' +
        'X_2 * 0.01), 4) AS amount_notax, '
      
        '                      dbo.Data0022.PACKING_SLIP_FLAG, dbo.Data00' +
