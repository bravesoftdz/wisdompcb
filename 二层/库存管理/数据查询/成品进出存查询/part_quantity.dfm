  object Aq53: TADOQuery
    Connection = Form1.ADOConnection1
    CursorType = ctStatic
    Filtered = True
    LockType = ltReadOnly
    Parameters = <>
    Prepared = True
    SQL.Strings = (
      
        'SELECT TOP 100 PERCENT dbo.Data0008.PROD_CODE, data0010.cust_cod' +
        'e as cust_code10,data0025.ORIG_CUSTOMER,'
      
        '      dbo.Data0010.ABBR_NAME AS abbr_name10, SUM(dbo.Data0053.QT' +
        'Y_ON_HAND) '
      
        '      AS quantity_hand,dbo.Data0025.ANALYSIS_CODE_2, dbo.Data002' +
        '5.SAMPLE_NR,'
      
        '      ROUND(SUM(dbo.Data0053.QTY_ON_HAND * dbo.Data0025.unit_sq)' +
        ', 4)'
      
        '      AS so_mianji, SUM(dbo.Data0053.QTY_ON_HAND * dbo.Data0025.' +
        'LATEST_PRICE) '
      '      AS amount, dbo.Data0025.MANU_PART_NUMBER, '
      '      dbo.Data0025.MANU_PART_DESC,'
      
        '      CASE Data0025.TTYPE WHEN 0 THEN '#39#37327#20135#39' WHEN 1 THEN '#39#26679#26495#39' ELSE' +
        ' '#39#26410#23450#20041#39' END AS TTYPE'
      'FROM dbo.Data0015 INNER JOIN'
      
        '      dbo.Data0053 ON dbo.Data0015.RKEY = dbo.Data0053.WHSE_PTR ' +
        'INNER JOIN'
      
        '      dbo.Data0016 ON dbo.Data0053.LOC_PTR = dbo.Data0016.RKEY I' +
        'NNER JOIN'
      '      dbo.Data0008 INNER JOIN'
      
        '      dbo.Data0025 ON dbo.Data0008.RKEY = dbo.Data0025.PROD_CODE' +
        '_PTR ON '
      '      dbo.Data0053.CUST_PART_PTR = dbo.Data0025.RKEY INNER JOIN'
      
        '      dbo.Data0010 ON dbo.Data0025.CUSTOMER_PTR = dbo.Data0010.R' +
        'KEY'
      'WHERE (dbo.Data0053.QTY_ON_HAND > 0)'
      
        'GROUP BY dbo.Data0010.ABBR_NAME,data0010.cust_code, dbo.Data0008' +
        '.PROD_CODE, dbo.Data0025.ANALYSIS_CODE_2,'
      
        '      dbo.Data0025.MANU_PART_NUMBER, dbo.Data0025.SAMPLE_NR,dbo.' +
        'Data0025.MANU_PART_DESC,Data0025.TTYPE,data0025.ORIG_CUSTOMER'
      'ORDER BY dbo.Data0025.MANU_PART_NUMBER')
    Left = 416
    Top = 456
