  object ADOQuery1: TADOQuery
    Connection = Form1.ADOConnection1
    CursorType = ctStatic
    Filtered = True
    LockType = ltReadOnly
    Parameters = <>
    Prepared = True
    SQL.Strings = (
      
        'SELECT TOP 100 PERCENT dbo.Data0008.PROD_CODE, dbo.Data0010.ABBR' +
        '_NAME,data0010.cust_code,'
      
        '      SUM(dbo.Data0052.QUAN_SHP) AS putout_total, dbo.Data0008.P' +
        'RODUCT_NAME,'
      
        '      ROUND(SUM(Data0052.QUAN_SHP * Data0025.unit_sq), 4) AS so_' +
        'mianqi,'
      
        '      ROUND(SUM(Data0052.QUAN_SHP * Data0064.REPORT_UNIT_VALUE1 ' +
        '* 0.001), 4) AS weight,'
      
        'ROUND(SUM(dbo.Data0052.QUAN_SHP * dbo.Data0060.parts_alloc / dbo' +
        '.Data0060.EXCH_RATE),'
      '       4) AS amount, dbo.Data0025.MANU_PART_NUMBER,'
      '      dbo.Data0025.MANU_PART_DESC, dbo.Data0025.ANALYSIS_CODE_2,'
      
        '      ROUND(SUM(dbo.Data0052.QUAN_SHP * dbo.Data0060.parts_alloc' +
        ' * (1 + dbo.Data0064.TAX_2 * 0.01) / dbo.Data0060.EXCH_RATE),'
      '       4) AS tax_amount,data0025.orig_customer,'
      
        'ROUND(sum(Data0052.QUAN_SHP * (Data0053.cost_pcs+Data0053.matl_o' +
        'vhd_pcs+'
      
        'Data0053.OVHD_COST+Data0053.PLANNED_QTY+data0053.ovhd1_pcs+data0' +
        '053.outsource_pcs)), 4) AS total_cost'
      'FROM dbo.data0439 RIGHT OUTER JOIN'
      '     dbo.Data0010 INNER JOIN'
      '     dbo.Data0060 INNER JOIN'
      '     dbo.Data0052 INNER JOIN'
      
        '     dbo.Data0064 ON dbo.Data0052.SO_SHP_PTR = dbo.Data0064.RKEY' +
        ' INNER JOIN'
      
        '     dbo.Data0053 ON dbo.Data0052.DATA0053_PTR = dbo.Data0053.RK' +
        'EY ON dbo.Data0060.RKEY = dbo.Data0064.SO_PTR INNER JOIN'
      '     dbo.Data0008 INNER JOIN'
      
        '     dbo.Data0025 ON dbo.Data0008.RKEY = dbo.Data0025.PROD_CODE_' +
        'PTR ON dbo.Data0060.CUST_PART_PTR = dbo.Data0025.RKEY ON'
      
        '     dbo.Data0010.RKEY = dbo.Data0060.CUSTOMER_PTR ON dbo.data04' +
        '39.rkey = dbo.Data0064.packing_list_ptr'
      'WHERE (dbo.Data0052.QUAN_SHP > 0)'
      'GROUP BY dbo.Data0008.PROD_CODE, dbo.Data0008.PRODUCT_NAME,'
      
        '      dbo.Data0010.ABBR_NAME,data0010.cust_code,dbo.Data0025.MAN' +
        'U_PART_NUMBER,'
      
        '      dbo.Data0025.MANU_PART_DESC, dbo.Data0025.ANALYSIS_CODE_2,' +
        'data0025.orig_customer')
    Left = 352
    Top = 280
