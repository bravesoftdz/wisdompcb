  object ADOquery1: TADODataSet
    Connection = ADOConnection1
    CursorType = ctStatic
    LockType = ltReadOnly
    CommandText = 
      'SELECT     TOP (100) PERCENT dbo.Data0053.RKEY, dbo.Data0053.WOR' +
      'K_ORDER_PTR,'#13#10' dbo.Data0006.WORK_ORDER_NUMBER, dbo.Data0053.CUST' +
      '_PART_PTR, '#13#10' dbo.Data0053.QTY_ON_HAND - dbo.Data0053.QTY_ALLOC ' +
      'AS qty_on_hand,'#13#10' dbo.Data0053.MFG_DATE, dbo.Data0016.LOCATION, ' +
      'dbo.Data0053.STEP, '#13#10' dbo.Data0025.MANU_PART_NUMBER, dbo.Data002' +
      '5.MANU_PART_DESC, dbo.data0492.WHOUSE_PTR'#13#10'FROM         dbo.Data' +
      '0016 INNER JOIN'#13#10'                      dbo.Data0053 INNER JOIN'#13#10 +
      '                      dbo.Data0025 ON dbo.Data0053.CUST_PART_PTR' +
      ' = dbo.Data0025.RKEY ON dbo.Data0016.RKEY = dbo.Data0053.LOC_PTR' +
      ' INNER JOIN'#13#10'                      dbo.Data0006 ON dbo.Data0053.' +
      'WORK_ORDER_PTR = dbo.Data0006.RKEY INNER JOIN'#13#10'                 ' +
      '     dbo.Data0010 ON dbo.Data0025.CUSTOMER_PTR = dbo.Data0010.RK' +
      'EY INNER JOIN'#13#10'                      dbo.data0492 ON dbo.Data000' +
      '6.CUT_NO = dbo.data0492.CUT_NO'#13#10'WHERE     (dbo.Data0053.QTY_ON_H' +
      'AND - dbo.Data0053.QTY_ALLOC > 0) '#13#10
