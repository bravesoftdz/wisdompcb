  object ado0022: TADOQuery
    Connection = Form1.ADOConnection1
    CursorType = ctStatic
    Filtered = True
    BeforeInsert = ado0022BeforeInsert
    BeforeEdit = ado0022BeforeEdit
    BeforeDelete = ado0022BeforeDelete
    Parameters = <>
    Prepared = True
    SQL.Strings = (
      
        'SELECT      dbo.Data0017.INV_PART_NUMBER, dbo.Data0017.INV_NAME,' +
        ' dbo.Data0017.INV_DESCRIPTION, dbo.Data0496.GROUP_DESC,dbo.data0' +
        '022.rkey as rkey22,'
      
        '  dbo.Data0023.ABBR_NAME AS code23, dbo.Data0022.QUAN_ON_HAND, d' +
        'bo.Data0022.QUAN_TO_BE_STOCKED, DATEDIFF(day, GETDATE(),'
      
        '  dbo.Data0022.EXPIRE_DATE) AS eff_day, dbo.Data0022.QUAN_ON_HAN' +
        'D - dbo.Data0022.QUAN_TO_BE_STOCKED AS quan_allow, dbo.Data0001.' +
        'CURR_CODE,'
      
        '  dbo.Data0022.tax_price, dbo.Data0016.LOCATION, dbo.Data0002.UN' +
        'IT_NAME, dbo.Data0022.EXPIRE_DATE, dbo.Data0022.BARCODE_ID, dbo.' +
        'Data0022.cust_decl,'
      
        '  dbo.Data0070.PO_REV_NO, dbo.Data0022.TAX_2, dbo.Data0456.GRN_N' +
        'UMBER, dbo.Data0015.WAREHOUSE_CODE, dbo.Data0456.ship_DATE, dbo.' +
        'Data0022.rohs,'
      
        '  dbo.Data0022.SUPPLIER2, dbo.Data0022.PRICE, ROUND(dbo.Data0022' +
        '.QUAN_ON_HAND * dbo.Data0017.STOCK_SELL, 4) AS total_area, dbo.D' +
        'ata0456.exch_rate,'
      
        '  ROUND(dbo.Data0022.PRICE * dbo.Data0456.exch_rate, 4) AS L_no_' +
        'taxprice, dbo.Data0022.tax_price * dbo.Data0456.exch_rate AS L_t' +
        'ax_price,'
      
        '  ROUND(dbo.Data0022.tax_PRICE * dbo.Data0456.exch_rate * dbo.Da' +
        'ta0022.QUAN_ON_HAND/(1 + dbo.Data0022.TAX_2 * 0.01), 4) AS amoun' +
        't_notax,data0022.PACKING_SLIP_FLAG,'
      
        '  dbo.Data0022.tax_price * dbo.Data0456.exch_rate * dbo.Data0022' +
        '.QUAN_ON_HAND AS amount_tax, CASE WHEN DATEDIFF(day, GETDATE(),'
      
        '  dbo.Data0022.EXPIRE_DATE) < 0 THEN '#39#24050#36807#26399#39' ELSE '#39#26377#25928#39' END AS stat' +
        'e, dbo.Data0022.STOCK_BASE,data0022.spec_place,Data0019.inv_grou' +
        'p_name,'
      
        '  dbo.Data0022.STOCK_BASE * dbo.Data0022.QUAN_ON_HAND AS '#24211#23384#24635#37325', d' +
        'bo.Data0071.reason,Data0070.PO_NUMBER,data0022.TDATE,data0016.Rk' +
        'ey as rkey16,'
      '  case  when dbo.Data0456.ttype=1 then '#39#32534#30721#20837#24211#39' '
      '           when dbo.Data0456.ttype=2 then '#39#26434#39033#20837#24211#39' '
      '           when dbo.Data0456.ttype=3 then '#39#30452#25509#20837#24211#39'  '
      '           when dbo.Data0456.ttype=4 then '#39#35843#25300#20837#24211#39' '
      '           when dbo.Data0456.ttype=5 then '#39'VMI'#20837#24211#39'  end as stype,'
      
        'data0068.PO_REQ_NUMBER,dbo.Data0070.ANALYSIS_CODE_1, dbo.Data000' +
        '5.EMPLOYEE_NAME,  dbo.Data0070.CONFIRMATION_NUMBER'
      'FROM         dbo.Data0015 INNER JOIN'
      '  dbo.Data0023 INNER JOIN'
      '  dbo.Data0456 INNER JOIN'
      '  dbo.Data0022 INNER JOIN'
      
        '  dbo.Data0017 ON dbo.Data0022.INVENTORY_PTR = dbo.Data0017.RKEY' +
        ' INNER JOIN'
      
        '  dbo.Data0496 ON dbo.Data0017.GROUP_PTR = dbo.Data0496.RKEY ON ' +
        'dbo.Data0456.RKEY = dbo.Data0022.GRN_PTR INNER JOIN'
      
        '  dbo.Data0002 ON dbo.Data0022.LOCATION_PTR_FROM = dbo.Data0002.' +
        'RKEY ON dbo.Data0023.RKEY = dbo.Data0456.SUPP_PTR INNER JOIN'
      
        '  dbo.Data0016 ON dbo.Data0022.LOCATION_PTR = dbo.Data0016.RKEY ' +
        'ON dbo.Data0015.RKEY = dbo.Data0456.warehouse_ptr INNER JOIN'
      
        '  dbo.Data0001 ON dbo.Data0456.currency_ptr = dbo.Data0001.RKEY ' +
        'LEFT OUTER JOIN'
      
        '  dbo.Data0071 ON dbo.Data0022.SOURCE_PTR = dbo.Data0071.RKEY LE' +
        'FT OUTER JOIN'
      
        '   dbo.Data0019 ON dbo.Data0496.group_ptr = dbo.Data0019.rkey LE' +
        'FT OUTER JOIN'
      
        '  dbo.Data0070 ON dbo.Data0456.PO_PTR = dbo.Data0070.RKEY LEFT O' +
        'UTER JOIN'
      
        '  dbo.data0005 ON dbo.data0070.employee_pointer = dbo.data0005.r' +
        'key  LEFT OUTER JOIN'
      '  dbo.data0069 ON data0071.rkey69=data0069.rkey LEFT OUTER JOIN '
      '  data0068 on data0069.PURCH_REQ_PTR=data0068.rkey'
