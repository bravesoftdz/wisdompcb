  object ADOQuery2: TADOQuery
    Connection = frmmain.ADOConnection1
    Parameters = <
      item
        Name = 'number'
        DataType = ftString
        NumericScale = 255
        Precision = 255
        Size = 18
        Value = Null
      end>
    SQL.Strings = (
      
        'SELECT      dbo.Data0103.RKEY, dbo.Data0103.GL_ACC_NUMBER, dbo.D' +
        'ata0103.DB_CR, CASE WHEN d103_2.GL_DESCRIPTION IS NOT NULL '
      
        '                      THEN d103_2.GL_DESCRIPTION + '#39'-'#39' + d103_1.' +
        'GL_DESCRIPTION + '#39'-'#39' + data0103.GL_DESCRIPTION ELSE CASE WHEN d1' +
        '03_1.GL_DESCRIPTION IS NOT NULL '
      
        '                      THEN d103_1.GL_DESCRIPTION + '#39'-'#39' + data010' +
        '3.GL_DESCRIPTION ELSE data0103.GL_DESCRIPTION END END AS GL_DESC' +
        'RIPTION'
      'FROM         dbo.Data0103 LEFT OUTER JOIN'
      
        '                      dbo.Data0103 AS d103_1 ON dbo.Data0103.PAR' +
        'ENT_PTR = d103_1.RKEY LEFT OUTER JOIN'
      
        '                      dbo.Data0103 AS d103_2 ON d103_1.PARENT_PT' +
        'R = d103_2.RKEY'
      'WHERE     (dbo.Data0103.GL_ACC_NUMBER = :number)')
    Left = 504
    Top = 400
  end
  object ADOQuery1: TADOQuery
    Connection = frmmain.ADOConnection1
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'rkey'
        Attributes = [paSigned]
        DataType = ftInteger
        Precision = 10
        Size = 4
        Value = Null
      end>
    SQL.Strings = (
      
        'SELECT      dbo.Data0103.RKEY, dbo.Data0103.GL_ACC_NUMBER, dbo.D' +
        'ata0103.DB_CR,'
      ' CASE WHEN d103_2.GL_DESCRIPTION IS NOT NULL'
      
        '                      THEN d103_2.GL_DESCRIPTION + '#39'-'#39' + d103_1.' +
        'GL_DESCRIPTION + '#39'-'#39' + data0103.GL_DESCRIPTION ELSE CASE WHEN d1' +
        '03_1.GL_DESCRIPTION IS NOT NULL '
      
        '                      THEN d103_1.GL_DESCRIPTION + '#39'-'#39' + data010' +
        '3.GL_DESCRIPTION ELSE data0103.GL_DESCRIPTION END END AS GL_DESC' +
        'RIPTION'
      'FROM         dbo.Data0103 LEFT OUTER JOIN'
      
        '                      dbo.Data0103 AS d103_1 ON dbo.Data0103.PAR' +
        'ENT_PTR = d103_1.RKEY LEFT OUTER JOIN'
      
        '                      dbo.Data0103 AS d103_2 ON d103_1.PARENT_PT' +
        'R = d103_2.RKEY'
      'WHERE     (dbo.Data0103.rkey = :rkey)')
    Left = 472
    Top = 400
  end
