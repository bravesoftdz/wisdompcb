  object ADS06: TADODataSet
    Connection = frmMain.ADOConnection1
    CursorType = ctStatic
    Filtered = True
    LockType = ltBatchOptimistic
    CommandText = 
      'declare @TempTable table(selected bit)      '#13#10'  insert @TempTabl' +
      'e  values (0)'#13#10'SELECT selected,  D06.RKEY,  D06.wtype AS wtype06' +
      ', d34.DEPT_CODE, d34.DEPT_NAME,d34.TTYPE, D06.QUAN_SCH, '#13#10'      ' +
      '  D06.WORK_ORDER_NUMBER, D06.PROD_STATUS, D06.QUAN_REJ,  D06.QUA' +
      'N_PROD, D06.PANELS,D56.TO_BE_CANCD,'#13#10'        D06.REFERENCE_PTR, ' +
      'd25.PARENT_PTR, D56.PANELS AS PNL_ONLine, d25.MANU_PART_NUMBER, ' +
      'd25.MANU_PART_DESC, D56.STEP, '#13#10'        D06.FLOW_NO AS Flow_NO_0' +
      '6, D56.QTY_BACKLOG, D56.TO_BE_STOCKED, D56.PUT_TYPE,D56.dept_ptr' +
      ','#13#10'        CASE D06.Prod_Status WHEN 3 THEN '#39#29983#20135#20013#39' WHEN 4 THEN '#39#22806 +
      #21457#29983#20135#39' WHEN 5 THEN '#39#24453#20837#20179#39' WHEN 6 THEN '#39#24453#20998#37197#39' WHEN 103 THEN '#39#29983#20135#20013#26242#32531#39' E' +
      'LSE '#39#39' END AS'#13#10'        prod_Status_CN, CASE isnull(D56.wType, 0)' +
      ' WHEN 0 THEN '#39#39' ELSE '#39#26159#39' END AS OnlineStatus_Old, '#13#10'        CASE' +
      ' D06.wtype WHEN 0 THEN '#39#27491#24120#39' WHEN 1 THEN '#39#34987#25286#20998#39' WHEN 2 THEN '#39#24453#36820#24037#39' ' +
      'WHEN 3 THEN '#39#36820#24037#39' WHEN 4 THEN '#39#24453#25253#24223#39' WHEN 5 THEN '#39#24050#25253#24223#39' ELSE '#39#39' END' +
      #13#10'        AS OnlineStatus, D06.CUTNO_WO, CASE isnull(D56.PUT_TYP' +
      'E, 0) WHEN 0 THEN '#39#39' ELSE '#39#26159#39' END AS ReCheck'#13#10'FROM         dbo.D' +
      'ata0025 AS d25 INNER JOIN'#13#10'                      dbo.Data0006 AS' +
      ' D06 ON D06.BOM_PTR = d25.RKEY INNER JOIN'#13#10'                     ' +
      ' dbo.Data0056 AS D56 ON D56.WO_PTR = D06.RKEY INNER JOIN'#13#10'      ' +
      '                dbo.Data0034 AS d34 ON D56.DEPT_PTR = d34.RKEY  ' +
      'join '#13#10'                      @TempTable on 1=1'#13#10'WHERE 1=1 '#13#10#13#10
