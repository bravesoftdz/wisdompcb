        Name = 'WH'
        Size = -1
        Value = Null
      end
      item
        Name = 'CID'
        Size = -1
        Value = Null
      end>
    SQL.Strings = (
      
        'select  WORK_ORDER_NUMBER,MANU_PART_NUMBER,FM_DEPT_PTR,dept_code' +
        ',dept_name,outtime,stackNum,Ttime,sum(PANELS)PANELS,sum(RTime)RT' +
        'ime'
      'from '
      '(SELECT   d6.WORK_ORDER_NUMBER,d25.MANU_PART_NUMBER, '
      
        'd48.FM_DEPT_PTR , d34.dept_code,d34.dept_name, d48.PANELS ,conve' +
        'rt(varchar(120),d48.outtime,120) as outtime , '
      ' isnull(d581.stacknum,0) as stackNum,'
      ' 0 as Ttime,'
      
        '  case when  isnull(d581.stacknum,0)>0  then isnull(d48.PANELS,0' +
        ')*isnull(d581.CTTime1,0)/isnull(d581.stacknum,0)/6  else 0 end a' +
        's  RTime'
      'FROM dbo.Data0048 d48'
      'INNER JOIN  dbo.Data0006 d6 ON d48.WO_PTR = d6.RKEY  '
      'inner join  data0034 d34  on d48.FM_DEPT_PTR=d34.rkey'
      'INNER JOIN dbo.Data0025 d25 ON d6.BOM_PTR = d25.RKEY  '
      'INNER JOIN  dbo.data0492 d492 ON d6.CUT_NO = d492.CUT_NO  '
      'INNER JOIN dbo.Data0010  d10 ON d492.COMPLETED = d10.RKEY  '
      
        'left join (select CTTime1,prodno,pid,stacknum  from   data0581 w' +
        'here cid=  :cid  ) d581  on  d25.rkey=d581.prodno and d34.rkey=d' +
        '581.pid'
      'WHERE   (d48.OUTTIME >=  :BDate)'
      'AND (d48.OUTTIME <=   :EDate )'
      'AND (d48.warehouse_ptr =  :WH) and d48.wtype<>3'
      
        'AND d48.FM_DEPT_PTR in (select pid from data0579 where cid=  :CI' +
        'D) '
      'and d48.PANELS>0'
      ')a '
      
        'group by WORK_ORDER_NUMBER,MANU_PART_NUMBER,FM_DEPT_PTR,dept_cod' +
        'e,dept_name,outtime,stackNum,Ttime')
    Left = 722
    Top = 267
  end
